  "private": false,
  "scripts": {
    "start": "parcel src/manifest.json"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-web-extension": "^1.6.1",
    "prettier": "^2.1.2",
    "react-router-dom": "^5.2.0"
